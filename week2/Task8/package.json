{
  "name": "type-safe-validator",
  "version": "1.0.0",
  "description": "A comprehensive type-safe validation library for TypeScript",
  "main": "dist/src/schema.js",
  "types": "dist/src/schema.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/schema.ts",
    "example": "ts-node examples/example.ts",
    "demo": "ts-node src/schema.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:check": "jest --coverage --coverageReporters=text-summary",
    "compile": "node dist/src/schema.js"
  },
  "keywords": ["validation", "typescript", "type-safe", "schema"],
  "author": "Developer",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
} 